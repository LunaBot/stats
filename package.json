{
  "name": "@lunabot/stats",
  "version": "0.0.1",
  "description": "",
  "main": "dist/client.js",
  "types": "dist/client.d.ts",
  "bin": {
    "register": "dist/register.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "start": "node ./dist/server.js",
    "start-debug-server": "tsup ./src/server.ts > /dev/null && LOG_LEVEL=silly LOG_TRANSPORT=console node dist/server.js",
    "start-debug-client": "tsup ./src/demo.ts > /dev/null && LOG_LEVEL=silly LOG_TRANSPORT=console node dist/demo.js",
    "build": "npm run build-client && npm run build-server && npm run build-demo && npm run build-register",
    "build-client": "./node_modules/.bin/tsup ./src/client.ts --dts",
    "build-server": "./node_modules/.bin/tsup ./src/server.ts",
    "build-demo": "./node_modules/.bin/tsup ./src/demo.ts",
    "build-register": "./node_modules/.bin/tsup ./src/register.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LunaBot/stats.git"
  },
  "keywords": [],
  "author": "Alexis Tyler <xo@wvvw.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LunaBot/stats/issues"
  },
  "homepage": "https://github.com/LunaBot/stats#readme",
  "dependencies": {
    "apollo-server-express": "^2.21.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "discord.js": "^12.5.1",
    "enmap": "^5.8.4",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-jit": "^0.5.0",
    "graphql-rate-limit": "github:OmgImAlexis/graphql-rate-limit#master",
    "graphql-tools": "^7.0.4",
    "graphql-ws": "^4.2.2",
    "uuid-apikey": "^1.5.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "eslint": "^7.22.0",
    "nodemon": "^2.0.7",
    "tsup": "^4.8.18",
    "typescript": "^4.2.3"
  }
}
